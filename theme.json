{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "layout": {
            "contentSize": "1140px",
            "wideSize": "100%"
        },
        "color": {
            "duotone": [],
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "link": true,
            "palette": [
                {
                    "slug": "turquoise",
                    "color": "#086788",
                    "name": "Turquoise"
                },
                {
                    "slug": "green",
                    "color": "#cbd334",
                    "name": "Green"
                },
                {
                    "slug": "gray",
                    "color": "#666666",
                    "name": "Gray"
                }
            ]
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": null,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "typography": {
            "lineHeight": true,
            "fontFamilies": [
                {
                    "fontFamily": "Montserrat,sans-serif",
                    "slug": "montserrat",
                    "name": "Montserrat"
                }
            ]
        }
    }
}
